{
  StringBuilder sb=new StringBuilder();
  sb.append(File.separator);
  String[] fileString=iconName.split(File.separator);
  sb.append(fileString[0]);
  sb.append(File.separator);
  sb.append(PATH_DRAWABLE_PREFIX);
  sb.append(size);
  sb.append(File.separator);
  sb.append(fileName);
  return sb.toString();
}
