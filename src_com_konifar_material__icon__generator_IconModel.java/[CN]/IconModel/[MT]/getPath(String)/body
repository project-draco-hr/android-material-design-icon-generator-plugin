{
  if (iconName != null) {
    String[] fileString=iconName.split(File.separator);
    StringBuilder sb=new StringBuilder();
    sb.append(File.separator);
    sb.append(PATH_ICONS);
    sb.append(File.separator);
    sb.append(fileString[0]);
    sb.append(getDrawabaleIconPath(fileName,size));
    return sb.toString();
  }
 else {
    return "";
  }
}
