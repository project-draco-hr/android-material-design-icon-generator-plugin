{
  InputStream is=getClass().getResourceAsStream(originalPath);
  OutputStream os=new FileOutputStream(destFile);
  int len=-1;
  byte[] b=new byte[1000 * 1024];
  try {
    while ((len=is.read(b,0,b.length)) != -1) {
      os.write(b,0,len);
    }
    os.flush();
  }
  finally {
    if (os != null)     try {
      os.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
