{
  InputStream is=null;
  OutputStream os=null;
  try {
    is=getClass().getResourceAsStream(fileName);
    File targetFile=new File(fileName);
    os=new FileOutputStream(targetFile);
    byte[] buffer=new byte[8 * 1024];
    int bytesRead;
    while ((bytesRead=is.read(buffer)) != -1) {
      os.write(buffer,0,bytesRead);
    }
    return targetFile;
  }
 catch (  FileNotFoundException e) {
    e.printStackTrace();
    return null;
  }
catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
 finally {
    if (os != null)     try {
      os.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
    if (is != null)     try {
      is.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
